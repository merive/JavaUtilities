<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>
<GridPane xmlns:fx="http://javafx.com/fxml/1" alignment="center" hgap="10" prefHeight="177.0" prefWidth="525.0"
          styleClass="background" stylesheets="@style.css" vgap="10"
          xmlns="http://javafx.com/javafx/10.0.2-internal" fx:controller="Controller">
    <columnConstraints>
        <ColumnConstraints/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints/>
    </rowConstraints>
    <Pane fx:id="Pane" prefHeight="191.0" prefWidth="526.0" styleClass="background" stylesheets="@style.css">
        <Button layoutX="180.0" layoutY="100.0" mnemonicParsing="false" onAction="#Run" prefHeight="28.0"
                prefWidth="161.0" styleClass="textfield" stylesheets="@style.css" text="OK">
            <font>
                <Font name="Droid Sans Bold" size="12.0"/>
            </font>
        </Button>
        <Label fx:id="res" layoutX="17.0" layoutY="139.0" prefHeight="17.0" prefWidth="487.0" text="And it's:">
            <font>
                <Font size="15.0"/>
            </font>
        </Label>
        <Label layoutX="113.0" layoutY="25.0" text="Enter the names (Separated by space) and press ENTER."/>
        <TextField fx:id="text" layoutX="16.0" layoutY="54.0" prefHeight="25.0" prefWidth="487.0" styleClass="textfield"
                   stylesheets="@style.css"/>
    </Pane>
</GridPane>
