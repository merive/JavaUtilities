<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="500.0" prefWidth="820.0"
            xmlns="http://javafx.com/javafx/11.0.1" fx:controller="com.merive.LauncherController">
    <Pane prefHeight="500.0" prefWidth="820.0" style="-fx-background-color: #1B262C;">
        <Button fx:id="startButton" alignment="CENTER" contentDisplay="CENTER" layoutX="206.0" layoutY="413.0"
                mnemonicParsing="false" onAction="#clickStartButton" prefHeight="74.0" prefWidth="598.0"
                style="-fx-background-color: #bbe1fa; -fx-border-color: #0f4c75; -fx-border-width: 4px;" text="Start"
                textAlignment="CENTER" textFill="#0f4c75" underline="true">
            <font>
                <Font name="Droid Sans Mono" size="24.0"/>
            </font>
        </Button>
        <ImageView fx:id="previewImage" fitHeight="331.0" fitWidth="598.0" layoutX="206.0" layoutY="73.0"
                   pickOnBounds="true" preserveRatio="true">
            <Image url="@background.jpg"/>
        </ImageView>
        <ImageView fx:id="launcherImage" fitHeight="200.0" fitWidth="200.0" layoutX="400.0" layoutY="139.0" pickOnBounds="true"
                   preserveRatio="true">
            <Image url="@settings.png"/>
        </ImageView>
        <ImageView fx:id="randomNameImage" fitHeight="200.0" fitWidth="200.0" layoutX="400.0" layoutY="139.0" pickOnBounds="true"
                   preserveRatio="true" visible="false">
            <Image url="@random_name_icon.png"/>
        </ImageView>
        <ImageView fx:id="calculatorImage" fitHeight="200.0" fitWidth="200.0" layoutX="400.0" layoutY="139.0" pickOnBounds="true"
                   preserveRatio="true" visible="false">
            <Image url="@calculator_icon.png"/>
        </ImageView>
        <ImageView fx:id="mailImage" fitHeight="200.0" fitWidth="200.0" layoutX="400.0" layoutY="139.0" pickOnBounds="true"
                   preserveRatio="true" visible="false">
            <Image url="@email.png"/>
        </ImageView>
        <ImageView fx:id="watchImage" fitHeight="200.0" fitWidth="200.0" layoutX="400.0" layoutY="139.0" pickOnBounds="true"
                   preserveRatio="true" visible="false">
            <Image url="@clock.png"/>
        </ImageView>
        <ImageView fx:id="RRCImage" fitHeight="200.0" fitWidth="200.0" layoutX="400.0" layoutY="139.0" pickOnBounds="true"
                   preserveRatio="true" visible="false">
            <Image url="@key.png"/>
        </ImageView>
        <Label fx:id="label" alignment="CENTER" contentDisplay="CENTER" layoutX="206.0" layoutY="14.0" prefHeight="50.0"
               prefWidth="598.0" text="Welcome" textFill="#0f4c75" underline="true">
            <font>
                <Font name="Droid Sans Mono" size="28.0"/>
            </font>
        </Label>
    </Pane>
    <Pane prefHeight="500.0" prefWidth="194.0" style="-fx-background-color: #1B262C;">
        <Button fx:id="randomNameButton" alignment="CENTER" contentDisplay="CENTER" layoutX="26.0" layoutY="68.0"
                mnemonicParsing="false" onAction="#clickRandomName" prefHeight="50.0" prefWidth="142.0"
                style="-fx-background-color: #bbe1fa; -fx-border-color: #0f4c75; -fx-border-width: 4px;"
                text="RandomName" textAlignment="CENTER" textFill="#0f4c75" underline="true">
            <font>
                <Font name="Droid Sans Mono" size="16.0"/>
            </font>
        </Button>
        <Button fx:id="calculatorButton" alignment="CENTER" contentDisplay="CENTER" layoutX="26.0" layoutY="138.0"
                mnemonicParsing="false" onAction="#clickCalculator" prefHeight="50.0" prefWidth="142.0"
                style="-fx-background-color: #bbe1fa; -fx-border-color: #0f4c75; -fx-border-width: 4px;"
                text="Calculator" textFill="#0f4c75" underline="true">
            <font>
                <Font name="Droid Sans Mono" size="16.0"/>
            </font>
        </Button>
        <Button fx:id="mailButton" alignment="CENTER" contentDisplay="CENTER" layoutX="26.0" layoutY="208.0"
                mnemonicParsing="false" onAction="#clickMail" prefHeight="50.0" prefWidth="142.0"
                style="-fx-background-color: #bbe1fa; -fx-border-color: #0f4c75; -fx-border-width: 4px;" text="Mail"
                textFill="#0f4c75" underline="true">
            <font>
                <Font name="Droid Sans Mono" size="16.0"/>
            </font>
        </Button>
        <Button fx:id="watchButton" alignment="CENTER" contentDisplay="CENTER" layoutX="26.0" layoutY="278.0"
                mnemonicParsing="false" onAction="#clickWatch" prefHeight="50.0" prefWidth="142.0"
                style="-fx-background-color: #bbe1fa; -fx-border-color: #0f4c75; -fx-border-width: 4px;" text="Watch"
                textFill="#0f4c75" underline="true">
            <font>
                <Font name="Droid Sans Mono" size="16.0"/>
            </font>
        </Button>
        <Button fx:id="RRCButton" alignment="CENTER" contentDisplay="CENTER" layoutX="26.0" layoutY="348.0"
                mnemonicParsing="false" onAction="#clickRRC" prefHeight="50.0" prefWidth="142.0"
                style="-fx-background-color: #bbe1fa; -fx-border-color: #0f4c75; -fx-border-width: 4px;" text="RRC"
                textFill="#0f4c75" underline="true">
            <font>
                <Font name="Droid Sans Mono" size="16.0"/>
            </font>
        </Button>
        <Button fx:id="exitButton" alignment="CENTER" contentDisplay="CENTER" layoutX="10.0" layoutY="413.0"
                mnemonicParsing="false" onAction="#exit" prefHeight="74.0" prefWidth="174.0"
                style="-fx-background-color: #bbe1fa; -fx-border-color: #0f4c75; -fx-border-width: 4px;" text="Exit"
                textFill="#0f4c75" underline="true">
            <font>
                <Font name="Droid Sans Mono" size="24.0"/>
            </font>
        </Button>
        <Label id="name_label" alignment="CENTER" contentDisplay="CENTER" layoutX="26.0" layoutY="8.0" prefHeight="50.0"
               prefWidth="142.0" text="MTools" textAlignment="CENTER" textFill="#0f4c75" underline="true">
            <font>
                <Font name="Droid Sans Mono" size="28.0"/>
            </font>
        </Label>
    </Pane>
</AnchorPane>
